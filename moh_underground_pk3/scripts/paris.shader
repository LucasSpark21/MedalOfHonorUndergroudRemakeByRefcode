textures/paris/eiffel_tower
{
qer_keyword stone
surfaceparm stone
qer_keyword window
qer_keyword masked
qer_keyword m5
{
map textures/paris/eiffel_tower.tga
  alphaFunc GE128
  depthWrite
nextbundle
  map $lightmap
 }
}

