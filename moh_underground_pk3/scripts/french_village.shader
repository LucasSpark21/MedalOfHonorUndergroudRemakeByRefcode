textures/french_village/affiche_1
{
    qer_keyword rock
    qer_keyword wall
    surfaceparm stone
    {
        map textures/french_village/affiche_1.tga
        depthWrite
        rgbGen identity
    }
    {
        map $lightmap
        rgbGen identity
        blendFunc GL_DST_COLOR GL_ZERO
        depthFunc equal
    }
}

